{
  "name": "Chrome Extension Webpack",
  "options_page": "options.html",
  "background": {
    "scripts":["background.js"],
    "persistent":false
  },
  
  "content_scripts": [
    {
      "matches": ["https://google.com/*"],
      "js": ["contentScript.bundle.js"]
    }
  ],
  "persimssions":[ "activeTab", "tabs", "contextMenus"],
  "manifest_version": 2
}
